generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Booking {
  id          Int      @id @default(autoincrement())
  name        String
  email       String
  phone       String
  serviceId   String
  serviceName String
  price       Int
  startAt     DateTime @unique
  createdAt   DateTime @default(now())
}